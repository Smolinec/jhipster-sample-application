<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Value.
    -->
    <changeSet id="20210131170002-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="temperature_id"
                                 baseTableName="value"
                                 constraintName="fk_value_temperature_id"
                                 referencedColumnNames="id"
                                 referencedTableName="temperature"/>

        <addForeignKeyConstraint baseColumnNames="value_id"
                                 baseTableName="value_temperature"
                                 constraintName="fk_value_temperature_value_id"
                                 referencedColumnNames="id"
                                 referencedTableName="value"/>

        <addForeignKeyConstraint baseColumnNames="temperature_id"
                                 baseTableName="value_temperature"
                                 constraintName="fk_value_temperature_temperature_id"
                                 referencedColumnNames="id"
                                 referencedTableName="temperature"/>
        
    </changeSet>
</databaseChangeLog>
